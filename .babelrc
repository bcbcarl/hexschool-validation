{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    [
      "ramda",
      {
        "useES": true
      }
    ],
    "@babel/plugin-transform-react-constant-elements",
    "transform-react-remove-prop-types",
    "styled-components"
  ]
}
