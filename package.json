    "postcss-normalize": "4.0.0",
    "prettier": "1.13.4",
    "prettier-eslint-cli": "4.7.1",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "style-loader": "0.21.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-no-unsupported-browser-features": "3.0.0",
    "webpack": "4.10.2",
